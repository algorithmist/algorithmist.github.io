digraph {
  splines=true;
  rankdir=LR;
  root [id="root"];
  a [id="a"];
  ab [id="ab"];
  aba [id="aba"];
  b [id="b"];
  ba [id="ba"];
  bab [id="bab"];
  root -> a [xlabel="a"];
  a -> ab [xlabel="b"];
  ab -> aba [xlabel="a"];
  root -> b [xlabel="b"];
  b -> ba [xlabel="a"];
  ba -> bab [xlabel="b"];
  root -> root [xlabel="[^ab]  "];
}
